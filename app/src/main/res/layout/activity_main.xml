<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:gravity="center_horizontal"
    tools:context="com.study.benchmarkorm.MainActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:baselineAligned="false">
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:layout_gravity="center"
            android:gravity="center"
            android:orientation="vertical">
            <Button
                android:id="@+id/button_write_simple"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Test write (simple)" />

            <TextView
                android:id="@+id/tv_write_simple"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                tools:text="200ms"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:layout_gravity="center"
            android:gravity="center"
            android:orientation="vertical">
            <Button
                android:id="@+id/button_write_complex"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Test write (complex)" />

            <TextView
                android:id="@+id/tv_write_complex"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                tools:text="200ms"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:layout_gravity="center"
            android:gravity="center"
            android:orientation="vertical">
            <Button
                android:id="@+id/button_write_balanced"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Test write (balanced)" />

            <TextView
                android:id="@+id/tv_write_balanced"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                tools:text="200ms"/>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:baselineAligned="false">
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:layout_gravity="center"
            android:gravity="center"
            android:orientation="vertical">
            <Button
                android:id="@+id/button_read_simple"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Test read (simple)" />

            <TextView
                android:id="@+id/tv_read_simple"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                tools:text="200ms"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:layout_gravity="center"
            android:gravity="center"
            android:orientation="vertical">
            <Button
                android:id="@+id/button_read_complex"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Test read (complex)" />

            <TextView
                android:id="@+id/tv_read_complex"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                tools:text="200ms"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:layout_gravity="center"
            android:gravity="center"
            android:orientation="vertical">
            <Button
                android:id="@+id/button_read_balanced"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Test read (balanced)" />

            <TextView
                android:id="@+id/tv_read_balanced"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                tools:text="200ms"/>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:baselineAligned="false">
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:layout_gravity="center"
            android:gravity="center"
            android:orientation="vertical">
            <Button
                android:id="@+id/button_update_simple"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Test update (simple)" />

            <TextView
                android:id="@+id/tv_update_simple"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                tools:text="200ms"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:layout_gravity="center"
            android:gravity="center"
            android:orientation="vertical">
            <Button
                android:id="@+id/button_update_complex"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Test update (complex)" />

            <TextView
                android:id="@+id/tv_update_complex"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                tools:text="200ms"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:layout_gravity="center"
            android:gravity="center"
            android:orientation="vertical">
            <Button
                android:id="@+id/button_update_balanced"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Test udpate (balanced)" />

            <TextView
                android:id="@+id/tv_update_balanced"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                tools:text="200ms"/>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:baselineAligned="false">
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:layout_gravity="center"
            android:gravity="center"
            android:orientation="vertical">
            <Button
                android:id="@+id/button_delete_simple"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Test delete (simple)" />

            <TextView
                android:id="@+id/tv_delete_simple"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                tools:text="200ms"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:layout_gravity="center"
            android:gravity="center"
            android:orientation="vertical">
            <Button
                android:id="@+id/button_delete_complex"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Test delete (complex)" />

            <TextView
                android:id="@+id/tv_delete_complex"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                tools:text="200ms"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:layout_gravity="center"
            android:gravity="center"
            android:orientation="vertical">
            <Button
                android:id="@+id/button_delete_balanced"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Test delete (balanced)" />

            <TextView
                android:id="@+id/tv_delete_balanced"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                tools:text="200ms"/>
        </LinearLayout>
    </LinearLayout>

</LinearLayout>
